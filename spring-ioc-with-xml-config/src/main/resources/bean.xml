<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="user" class="com.lazyben.entity.User" autowire="byName">
        <property name="name" value="Ben"/>
        <property name="id" value="1"/>
<!--        <property name="account" ref="account"/>-->
    </bean>
    <bean id="account1" class="com.lazyben.entity.Account">
        <property name="balance" value="100"/>
    </bean>
    <bean id="account2" class="com.lazyben.entity.Account">
        <property name="balance" value="200"/>
    </bean>
</beans>